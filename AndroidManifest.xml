<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marathonapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission
       android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
   <uses-permission
       android:name="android.permission.ACCESS_NETWORK_STATE"/>
   <uses-permission
       android:name="android.permission.INTERNET"/>
   <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
   
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
    	
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data android:name="com.google.android.gms.version"
          android:value="@integer/google_play_services_version" />
        <activity
            android:name="com.example.marathonapp.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.marathonapp.ListNonprofitsActivity"
            android:label="@string/title_activity_list_nonprofits" >
        </activity>
        <activity
            android:name="com.example.marathonapp.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="com.example.marathonapp.UserInfoActivity"
            android:label="@string/title_activity_user_info" >
        </activity>
        <activity
            android:name="com.example.marathonapp.SkillsActivity"
            android:label="@string/title_activity_skills" >
        </activity>
    </application>

</manifest>